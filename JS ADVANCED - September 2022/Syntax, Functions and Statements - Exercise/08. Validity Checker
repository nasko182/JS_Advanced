function func(x1,y1,x2,y2){

    function isValid (x1,y1,x2,y2){
        
        let first = Number(Math.pow((x2-x1),2));
        let second = Number(Math.pow((y2-y1),2));
        let result = Math.sqrt(first+second);


        if(result== result.toFixed(0)){
            console.log(`{${x1}, ${y1}} to {${x2}, ${y2}} is valid` )
        }
        else{
            console.log(`{${x1}, ${y1}} to {${x2}, ${y2}} is invalid` )
        }
    }

    isValid(x1,y1,0,0);
    isValid(x2,y2,0,0);
    isValid(x1,y1,x2,y2)


  }
