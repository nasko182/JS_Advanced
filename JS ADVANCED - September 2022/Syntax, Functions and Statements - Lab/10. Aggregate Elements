function func(x){

    let sum = 0;

    for(let i = 0; i< x.length;i++){
        sum+=x[i];
    }

    let smth = 0;

    for(let i = 0; i< x.length;i++){
        smth+=1/x[i];
    }

    let concat = ``;
    for(let i =0;i<x.length;i++){
        concat+=x[i];
    }

    console.log(sum);
    console.log(smth)
    console.log(concat)
  }
