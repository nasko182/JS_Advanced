class Circle{
    constructor(radius){
        this.radius=radius;
        this.diameter=radius*2;
    }

    get diameter(){
        return this._diameter;
    }
    set diameter(value){
        this.radius=value/2;
        this._diameter=value;
    }

    get area(){
        return Math.PI*(this.radius*this.radius);
    }
}
